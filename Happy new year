SELECT geometry::STLineFromText('LINESTRING(7.6 0.2, 7.6 1)', 0).STBuffer(0.03)
union all
SELECT geometry::Point(7.6, 0, 0).STBuffer(0.07)
UNION ALL
SELECT geometry::STLineFromText('LINESTRING(7.8 0.2, 7.8 1)', 0).STBuffer(0.03)
union all
SELECT geometry::Point(7.8, 0, 0).STBuffer(0.07)
UNION ALL
SELECT geometry::STLineFromText('LINESTRING(8 0.2, 8 1)', 0).STBuffer(0.03)
union all
SELECT geometry::Point(8, 0, 0).STBuffer(0.07)
