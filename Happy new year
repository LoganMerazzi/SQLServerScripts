SELECT geometry::STLineFromText('LINESTRING(0 0, 0 1)', 0).STBuffer(0.01)
UNION ALL
SELECT geometry::STLineFromText('LINESTRING(0 1, 0.5 1)', 0).STBuffer(0.01)
UNION ALL
SELECT geometry::STLineFromText('LINESTRING(0 0.6, 0.5 0.6)', 0).STBuffer(0.01)
UNION ALL 
SELECT geometry::STLineFromText('LINESTRING(0.6 0, 0.6 1)', 0).STBuffer(0.01)
UNION ALL 
SELECT geometry::STLineFromText('LINESTRING(0.6 1, 1.1 1)', 0).STBuffer(0.01)
UNION ALL 
SELECT geometry::STLineFromText('LINESTRING(0.6 0.5, 1.1 0.5)', 0).STBuffer(0.01)
UNION ALL 
SELECT geometry::STLineFromText('LINESTRING(0.6 0, 1.1 0)', 0).STBuffer(0.01)
UNION ALL 
SELECT geometry::STLineFromText('LINESTRING(1.2 0, 1.2 1)', 0).STBuffer(0.01)
UNION ALL 
SELECT geometry::STLineFromText('LINESTRING(1.2 0, 1.7 0)', 0).STBuffer(0.01)
UNION ALL 
SELECT geometry::STLineFromText('LINESTRING(1.8 0, 1.8 1)', 0).STBuffer(0.01)
UNION ALL
